{
  "name": "@lexjs/web-cli",
  "version": "0.0.5",
  "description": "CLI for making web searches from a terminal",
  "main": "./dist/bundle.js",
  "exports": "./dist/bundle.js",
  "scripts": {
    "compile": "rm -rf ./lib && npx tsc",
    "bundle": "rm -rf ./dist && webpack",
    "build": "npm run compile && npm run bundle",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "websearch",
    "web-search",
    "search",
    "search-engine",
    "cli"
  ],
  "author": "Lex Borisoff <hello@lexjs.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/LexBorisoff/web-cli.git"
  },
  "bin": {
    "web": "dist/bundle.js"
  },
  "files": [
    "/dist"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "open": "^8.4.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/node": "^18.17.6",
    "@types/open": "^6.2.1",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^3.4.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-shebang-plugin": "^1.1.8"
  }
}
