{
  "name": "@lexjs/web-cli",
  "version": "1.2.0",
  "description": "CLI for making browser search queries",
  "type": "module",
  "scripts": {
    "prebuild": "rm -fr ./dist",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Lex Borisoff <lex@borisoff.dev>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LexBorisoff/web-cli.git"
  },
  "homepage": "https://github.com/LexBorisoff/web-cli#readme",
  "bugs": {
    "url": "https://github.com/LexBorisoff/web-cli/issues"
  },
  "exports": {
    "./config": {
      "types": "./dist/types/config/index.d.ts",
      "import": "./dist/config/index.js",
      "default": "./dist/config/index.js"
    },
    "./core": {
      "types": "./dist/types/core/index.d.ts",
      "import": "./dist/core/index.js",
      "default": "./dist/core/index.js"
    }
  },
  "bin": {
    "web": "dist/cli.js"
  },
  "keywords": [
    "cli",
    "search",
    "websearch",
    "web-search",
    "search-engine",
    "browser-query",
    "web-query",
    "browser",
    "google"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "open": "^9.1.0",
    "prompts": "^2.4.2",
    "type-fest": "^4.15.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/node": "^18.17.6",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^3.4.1",
    "typescript": "^5.4.5"
  }
}
